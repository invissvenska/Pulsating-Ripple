<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PulsatingLayout">
        <attr name="pr_color" format="color" />
        <attr name="pr_strokeWidth" format="dimension"/>
        <attr name="pr_radius" format="dimension"/>
        <attr name="pr_duration" format="integer"/>
        <attr name="pr_rippleAmount" format="integer"/>
        <attr name="pr_scale" format="float"/>
        <attr name="pr_type" format="enum">
            <enum name="fillRipple" value="0"/>
            <enum name="strokeRipple" value="1"/>
        </attr>
    </declare-styleable>
</resources>